{
  "extends": ["stylelint-config-standard", "stylelint-config-clean-order"],
  "rules": {
    "color-hex-length": "long",
    "selector-class-pattern": null,
    "no-descending-specificity": null,
    "order/order": [
      [
        "custom-properties",
        "declarations"
      ],
      {
        "disableFix": true,
        "unspecified": "ignore"
      }
    ]
  }
}


